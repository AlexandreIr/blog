<%- include('../../partials/header.ejs'); %>
<body>
    <%- include('../../partials/homenavbar') %>
    <div class="container">
        <br>
        <% result.articles.rows.forEach(article=>{ %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title %></h2>
                    <br>
                </div>
                <div class="card-body">
                    <a href="/<%= article.slug %>" class="btn btn-success">Ler artigo</a>
                </div>
            </div>
            <br>
        <% }) %>
    </div>
</body>

